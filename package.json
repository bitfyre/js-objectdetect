{
  "name": "object-detect",
  "version": "2.0.0-alpha.1",
  "description": "Native JavaScript Object Detection",
  "main": "main.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build:cjs": "NODE_ENV=cjs rollup --config",
    "build:es": "NODE_ENV=es rollup --config",
    "build:iife:min": "NODE_ENV=production rollup --config",
    "build:iife": "NODE_ENV=development rollup --config",
    "build":
      "npm run build:iife && npm run build:iife:min && npm run build:cjs && npm run build:es ",
    "start": "rollup --config --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix main.js src/**/*.js rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitfyre/object-detect.git"
  },
  "keywords": ["face-detection", "object-detection"],
  "contributors": [
    {
      "name": "Martin Tschirsich"
    },
    {
      "name": "Alex Lemanski",
      "url": "http://bitfyre.net"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitfyre/object-detect/issues"
  },
  "homepage": "https://github.com/bitfyre/object-detect#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.8.2",
    "rollup": "^0.52.0",
    "rollup-plugin-eslint": "^4.0.0"
  }
}
